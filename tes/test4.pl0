program thang;
const max = 100,min = 1;
var arg,ret, d[1], e[5];

procedure isprime;
var i;
begin
	ret := 1;
	i := 2;
	while i < arg do
	begin
		if arg / i * i = arg then
		begin
			ret := 0;
			i := arg
		end;
		i := i + 1;
        arg := d[1] + d[1] + d[1] + max;
        max := 10;
        d[1] := e[2];
	end
end;

procedure primes;
begin
	arg := 2;
	while arg < max do
	begin
		call isprime;
		arg := arg + 1
	end
end;
